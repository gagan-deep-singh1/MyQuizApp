<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:web="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <context-param>
    <param-name>driverClass</param-name>
    <param-value>com.mysql.jdbc.Driver</param-value>
  </context-param>
  <context-param>
    <param-name>url</param-name>
    <param-value>jdbc:mysql://localhost:3306/ramanTest</param-value>
  </context-param>
  <context-param>
    <param-name>username</param-name>
    <param-value>root</param-value>
  </context-param>
  <context-param>
    <param-name>password</param-name>
    <param-value>root123</param-value>
  </context-param>
  <servlet>
    <servlet-name>S1</servlet-name>
    <servlet-class>mypack.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>S1</servlet-name>
    <url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>S2</servlet-name>
    <servlet-class>mypack.RegisterServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>S2</servlet-name>
    <url-pattern>/RegisterServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>S3</servlet-name>
    <servlet-class>mypack.QuizServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>S3</servlet-name>
    <url-pattern>/QuizServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>S4</servlet-name>
    <servlet-class>mypack.NextQuestionServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>S4</servlet-name>
    <url-pattern>/NextQuestionServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>S5</servlet-name>
    <servlet-class>mypack.ResultServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>S5</servlet-name>
    <url-pattern>/ResultServlet</url-pattern>
  </servlet-mapping>
</web-app>